@model IEnumerable<prueba2.Models.contratos>

@{
    ViewData["Title"] = "contratos";
}

<h1>Lista de contratos</h1>

<p>
    <a asp-action="Create" class="btn btn-outline-info btn-rounded btn-block my-4 waves-effect z-depth-0">Crear nuevo</a>
</p>
<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.numerocontrato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.nombreentidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fechainicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fechafin)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.numerocontrato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nombreentidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fechainicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fechafin)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id" class="btn-warning btn">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.Id" class="btn-primary btn">Detalles</a> |
                <a asp-action="Delete" asp-route-id="@item.Id" class="btn-danger btn">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
